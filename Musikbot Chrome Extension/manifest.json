{
	"name": "Musikbot Extension",
	"version": "0.11",
	"description": "Schneller Lieder hinzuf√ºgen",
	"manifest_version": 2,
	"key": "",
	"options_ui": {
		"page": "ui/options.html",
		"open_in_tab": false
	},
	"permissions": [
		"identity",
		"declarativeContent",
		"activeTab",
		"https://id.elite12.de/auth/*"
	],
	"icons": {
		"16": "res/favicon_16.png",
		"48": "res/favicon_48.png",
		"128": "res/favicon_128.png"
	},
	"background": {
		"scripts": [
			"scripts/background.js",
			"thirdParty/jquery-3.3.1.min.js"
		],
		"persistent": false
	},
	"page_action": {
		"default_title": "Elite12 Musikbot",
		"default_popup": "ui/popup.html",
		"default_icons": {
			"16": "res/favicon_16.png",
			"48": "res/favicon_48.png",
			"128": "res/favicon_128.png"
		}
	}
}
